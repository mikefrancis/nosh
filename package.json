{
  "name": "@mikefrancis/nosh",
  "license": "MIT",
  "description": "Modern RSS reader",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "format": "prettier --list-different src",
    "lint": "eslint src",
    "test": "jest --rootDir=src --testEnvironment=jsdom"
  },
  "dependencies": {
    "@panelbear/panelbear-js": "1.3.3",
    "axios": "0.24.0",
    "dayjs": "1.10.7",
    "next": "12.0.7",
    "next-pwa": "5.4.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "redux": "4.1.2",
    "redux-batched-actions": "0.5.0",
    "redux-thunk": "2.4.0",
    "rss-parser": "3.12.0"
  },
  "devDependencies": {
    "@testing-library/react": "12.1.2",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.12",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/react-redux": "7.1.20",
    "autoprefixer": "10.4.0",
    "cypress": "8.7.0",
    "cypress-axe": "0.13.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "husky": "7.0.4",
    "jest": "27.4.5",
    "lint-staged": "11.2.6",
    "msw": "0.36.3",
    "postcss": "8.3.11",
    "postcss-nested": "5.0.6",
    "prettier": "2.4.1",
    "redux-logger": "3.0.6",
    "tailwindcss": "2.2.19",
    "typescript": "4.4.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
